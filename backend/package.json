{
	"author": "",
	"dependencies": {
		"express": "5.1.0"
	},
	"description": "A basic typescript app starter for 2023.",
	"devDependencies": {
		"@types/express": "~5.0.3",
		"@types/node": "^24.0.3",
		"husky": "^8.0.1",
		"vitest": "3.2.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pnpm run test && pnpm run format && pnpm run lint"
		}
	},
	"license": "ISC",
	"main": "index.ts",
	"name": "typescript-starter",
	"scripts": {
		"format": "biome format --write",
		"lint": "biome lint",
		"start": "node --env-file-if-exists=.env src/index.ts",
		"start:dev": "node --env-file-if-exists=.env --watch-path=./src src/index.ts",
		"test": "vitest --run --coverage",
		"test:dev": "vitest --watch"
	},
	"type": "module",
	"version": "1.0.0"
}
