{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"moduleDetection": "force",
		"noEmit": true,
		"rewriteRelativeImportExtensions": true,
		"types": [
			"node"
		]
	},
	"exclude": [
		"src/**/*.spec.ts"
	],
	"extends": [
		"@tsconfig/strictest/tsconfig.json",
		"@tsconfig/node24/tsconfig.json",
		"@tsconfig/node-ts/tsconfig.json"
	],
	"include": [
		"src/**/*"
	]
}
